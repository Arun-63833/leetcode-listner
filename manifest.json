{
  "manifest_version": 3,
  "name": "HTTP Spy Extension",
  "version": "1.0",
  "description": "Intercept HTTP responses",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
