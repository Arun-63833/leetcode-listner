{
  "manifest_version": 3,
  "name": "HTTP Spy Extension",
  "version": "1.0",
  "description": "Intercept HTTP responses",
  "permissions": ["scripting"],
  "host_permissions": ["https://leetcode.com/*"],
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["https://leetcode.com/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
